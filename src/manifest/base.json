{
  "name": "TabSearch",
  "version": "0.5.0",
  "description": "Easy tab search & switching",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "sessions"
  ],
  "optional_permissions": [
    "bookmarks",
    "history"
  ],
  "icons": {
    "48": "icons/logo-48.png",
    "96": "icons/logo-96.png"
  },
  "options_ui": {
    "page": "settings/index.html",
    "open_in_tab": true
  }
}
